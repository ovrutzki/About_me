// 1
const  FictionalPenguins = {
    character : 'roy and silo',
    origin : 'And Tango Makes Three',
    author : 'Peter Parnell'
}
console.log(`Hello, I'm a penguin and my name is ${FictionalPenguins.character}`);
FictionalPenguins.canFly = false;
FictionalPenguins.chrip = function() {
    console.log('CHIRP CHIRP! Is this what penguins sound like?');
}
// 6
FictionalPenguins.sayHello = function() {
    console.log(`Hello, I'm a penguin and my name is ${FictionalPenguins.character}`);
}
// console.log(FictionalPenguins);
FictionalPenguins.sayHello();
// 7
FictionalPenguins.character =  "Penguin McPenguinFace";
FictionalPenguins.sayHello();
// 8
FictionalPenguins.fly = function (){
    if (FictionalPenguins.canFly) {
       console.log('I can fly'); 
    } else {
        console.log('no fly for me');
    }
}
FictionalPenguins.fly();

FictionalPenguins.test = function(){
    for (let i = 0; i < Object.length; i++) {
        console.log('hello');
        
    }
}

test(FictionalPenguins);
